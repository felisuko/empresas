CREATE SEQUENCE JBRAVO.SQ_GERI_CONSUMIDOR_CUESTIONAR MINVALUE 1 MAXVALUE 9999999999 INCREMENT BY 1 START WITH 1 NOCACHE  ORDER  NOCYCLE ;

CREATE TABLE JBRAVO.GERI_CONSUMIDOR_CUESTIONARIO
(
	ID_SECUENCIA    NUMBER(13,0) NOT NULL ENABLE 
  ,	GEN_T_CONSUMIDOR	NUMBER(4)
  ,	GEN_C_CONSUMIDOR	NUMBER(4)
  ,	GEN_T_CUESTIONARIO	NUMBER(4)
  ,	GEN_C_CUESTIONARIO	NUMBER(4)
  ,	F_INACTIVO			DATE

  ,	CONSTRAINT PK_GERI_CONSUMIDOR_CUESTIONAR PRIMARY KEY (ID_SECUENCIA)
 )




-- CREA SECUENCIA
------------
CREATE SEQUENCE JBRAVO.SQ_GERI_RESPUESTAS_ACCIONES  MINVALUE 1 MAXVALUE 9999999999 INCREMENT BY 1 START WITH 1 CACHE 20 ORDER  NOCYCLE ; 

ALTER TABLE JBRAVO.GERI_RESPUESTAS_ACCIONES ADD( 
  GENESYS_ID_TIPO_ACCION NUMBER(4,0),
  ID_SECUENCIA       NUMBER(10,0)
); 


-- BORRA PK
ALTER TABLE JBRAVO.GERI_RESPUESTAS_ACCIONES DROP CONSTRAINT PK_GERI_RESPUESTAS_ACCIONES;
DROP INDEX PK_GERI_RESPUESTAS_ACCIONES;

-- CREA NUEVA PK
ALTER TABLE JBRAVO.GERI_RESPUESTAS_ACCIONES ADD CONSTRAINT PK_GERI_RESPUESTAS_ACCIONES PRIMARY KEY (ID_SECUENCIA);


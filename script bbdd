-- Eliminar sequence
drop sequence SQ_xxxxxx;

-- Create sequence 
create sequence SQ_xxxxxx
minvalue 1
maxvalue 999999999999999999999999999
start with 7724695
increment by 1
nocache
order;



CREATE INDEX JBRAVO.GENE_ORIGEN_TELEFONOS_ID33 ON JBRAVO.GENE_ORIGEN_TELEFONOS (ID_REPRESENTANTE);
CREATE INDEX JBRAVO.GENE_ORIGEN_TELEFONOS_ID33 ON JBRAVO.GENE_ORIGEN_TELEFONOS (ID_REPRESENTANTE)
  tablespace INDEX_JBRAVO
  pctfree 10
  initrans 2
  maxtrans 255
  storage
  (
    initial 64K
    next 1M
    minextents 1
    maxextents unlimited
  );

ALTER TABLE JBRAVO.GENE_ORIGEN_TELEFONOS ADD(
  ID_REPRESENTANTE       NUMBER(10)
);


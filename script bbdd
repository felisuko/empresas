	F_ALTA 		DATE, 
	F_ESTADO 	DATECREATE SEQUENCE JBRAVO.SQ_GERI_CONSUMIDOR_CUESTIONAR MINVALUE 1 MAXVALUE 9999999999 INCREMENT BY 1 START WITH 1 NOCACHE  ORDER  NOCYCLE ;

